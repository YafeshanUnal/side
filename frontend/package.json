{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"storybook": "concurrently \"yarn:watch:*\"",
		"build-storybook": "concurrently \"yarn:build:*\"",
		"build:css": "npx tailwindcss -i ./src/styles/globals.css -o ./build/tailwind-for-storybook.css",
		"build:storybook": "build-storybook -c .storybook -s public -s build",
		"watch:css": "npx tailwindcss -i ./src/styles/globals.css -o ./build/tailwind-for-storybook.css --watch",
		"watch:storybook": "start-storybook -p 4443 -c .storybook -s public -s build",
		"test:ci": "CI=true yarn test --coverage",
		"test": "jest --env=jsdom",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"prepare": "husky install"
	},
	"dependencies": {
		"@next/font": "13.1.6",
		"@reduxjs/toolkit": "^1.9.2",
		"@types/node": "18.13.0",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.10",
		"clsx": "^1.2.1",
		"cookies-next": "^2.1.1",
		"cors": "^2.8.5",
		"eslint": "8.34.0",
		"eslint-config-next": "13.1.6",
		"next": "13.1.6",
		"next-connect": "^0.13.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.42.1",
		"react-hot-toast": "^2.4.0",
		"react-redux": "^8.0.5",
		"react-responsive-carousel": "^3.2.23",
		"react-wheel-of-prizes": "^1.0.9",
		"typescript": "4.9.5"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@commitlint/cli": "^17.4.2",
		"@commitlint/config-conventional": "^17.4.2",
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-interactions": "^6.5.15",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/builder-webpack5": "^6.5.15",
		"@storybook/manager-webpack5": "^6.5.15",
		"@storybook/react": "^6.5.15",
		"@storybook/testing-library": "^0.0.13",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.2.5",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-mock-store": "^1.0.3",
		"autoprefixer": "^10.4.13",
		"babel-loader": "^8.3.0",
		"color": "^4.2.3",
		"concurrently": "^7.6.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-storybook": "^0.6.10",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"prettier-plugin-organize-imports": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"redux-mock-store": "^1.5.4",
		"tailwindcss": "^3.2.6",
		"ts-jest": "^29.0.5"
	}
}
